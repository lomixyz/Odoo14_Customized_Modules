<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_move_form_custom_warehouse" model="ir.ui.view">
        <field name="name">account.move.form.custom.warehouse</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet//group" position="inside">
                <group>
                    <field name="user_warehouse_id" readonly="1"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
